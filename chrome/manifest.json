{
  "name": "close google tabs",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Quickly close all Google Search tabs",
  "browser_action": {
    "default_icon": "icon_48.png"
  },
   "icons": {
      "128": "icon_128.png",
      "16": "icon_16.png",
      "48": "icon_48.png"
   },  
  "permissions": [  "tabs", "contextMenus", "http://*/*", "https://*/*" ],
  "background": { "scripts": ["main.js"] }
}
